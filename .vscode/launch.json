{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "bccm-mobile: debug",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "bccm-mobile: attach",
      "request": "attach",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "bccm-mobile: profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "cwd": "${workspaceFolder}",
      "program": "lib/main_prod.dart"
    },
    {
      "name": "bccm-mobile: release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "cwd": "${workspaceFolder}",
      "program": "lib/main_prod.dart"
    },
    {
      "name": "Debug build_runner",
      "cwd": "${workspaceFolder}/kids",
      "request": "launch",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "type": "dart",
      "args": [
        "build",
        "--delete-conflicting-outputs",
        "--verbose"
      ]
    },
    {
      "name": "kids-mobile: debug",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/kids",
      "args": [
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ],
      "program": "lib/main.dart"
    },
    {
      "name": "kids-mobile: profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "cwd": "${workspaceFolder}/kids",
      "args": [
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit",
        "--trace-skia"
      ],
      "program": "lib/main.dart"
    },
    {
      "name": "kids-mobile: release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "cwd": "${workspaceFolder}/kids",
      "args": [
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ],
      "program": "lib/main.dart"
    },
    {
      "name": "kids-mobile: attach",
      "request": "attach",
      "type": "dart",
      "cwd": "${workspaceFolder}/kids",
      "args": [
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ],
      "program": "lib/main.dart"
    },
    {
      "name": "kids-mobile: Debug build_runner",
      "cwd": "${workspaceFolder}/kids",
      "request": "launch",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "type": "dart",
      "args": [
        "build",
        "--delete-conflicting-outputs",
        "--verbose"
      ]
    }
  ]
}